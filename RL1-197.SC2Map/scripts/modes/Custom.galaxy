trigger gt_CustomGame;

bool SetOptions (){
    UIDisplayMessage(PlayerGroupActive(), c_messageAreaChat, StringToText("Setting Options"));

    return true;
}

bool gt_CustomGame_func (bool TestConds, bool RunActions){
    SetOptions();
    return true;
}

void gt_CustomGame_Init () {
    gt_CustomGame = TriggerCreate("gt_CustomGame_func");
    TriggerAddEventMapInit(gt_CustomGame);
}
