const int c_defaultHercules = 0;
const int c_medivacHercules = 0;

void InitializeHerculesSkins(){
    allSkinTypes[c_herculesSkinID].skins[c_killRequirement][c_defaultHercules].model = "Hercules";
    allSkinTypes[c_herculesSkinID].skins[c_killRequirement][c_defaultHercules].buttonPath = "Assets\\Textures\\btn-unit-terran-hercules.dds";

    allSkinTypes[c_herculesSkinID].skins[c_prizeRequirement][c_medivacHercules].model = "Medivac";
    allSkinTypes[c_herculesSkinID].skins[c_prizeRequirement][c_medivacHercules].buttonPath = "Assets\\Textures\\btn-unit-terran-medivac.dds";
}